{{ include('admin/header.twig') }}
                   <div class="user-content">
                      <div class="user-panel">
                        <div class="row">
                          <div class="col-md-12">
                            <h4>Manaage Patients</h4>
                            <a href="add-user" class="btn btn-small btn-primary"><em class="ti ti-plus"></em>Add New user</a>
                            <br /><br />
                            <table class="data-table tranx-table">
                              <tr>
                                <th>SN</th>
                                <th>Patient Name</th>
                                <th>Email</th>
                                <th>Year of Birth</th>
                                <th>Gender</th>
                                <th></th>
                              </tr>
                              {% set i = 1 %}
                              {% for pardon in pardoned %}
                              <tr class="text-wrap">
                                <td>{{ i }} </td>
                                 <td>{{ pardon.name }} </td>
                                 <td>{{ pardon.email }} </td>
                                <td>{{ pardon.gender }} </td>
                                <td>{{ pardon.yob }} </td>
                                <td><a href="diagnose/{{ pardon.id }}">Diagnose</a> </td>
                              </tr>
                              {% set i = i+1 %}
                              {% endfor %}
                            </table>

                        </div>
                        </div><!-- .row -->
                       
          
                    </div><!-- .user-panel -->
                </div><!-- .user-content -->
            </div><!-- .d-flex -->
        </div><!-- .container -->
    </div>
    <!-- UserWraper End -->
